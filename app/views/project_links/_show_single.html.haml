%div.flex.two.list{ 'data-' + show_single.js_name => show_single.id }
  %div
    = link_to show_single.name, show_single.url, :target => '_blank'
  %div
    = show_single.notes
  %span
    = link_to "E", edit_project_link_path(show_single), remote: true, class: 'edit_object'
    != "&nbsp&nbsp"
    = link_to "X", project_link_path(show_single, format: :js), method: :delete, data: { confirm: 'Are you certain you want to delete this?' }, remote: true, class: :remove_button
